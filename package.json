{
  "name": "electron-react-typescript",
  "version": "0.0.1",
  "description": "An app that does stuff",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack/react.webpack.js --mode development",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.js",
    "build:react": "webpack --config webpack/react.webpack.js && mv _redirects dist/renderer/_redirects",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir --win portable",
    "test": "jest"
  },
  "keywords": [],
  "author": "Ivan Palatov <ivanakazver3d@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/core": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.15",
    "@types/katex": "^0.11.0",
    "@types/mathjs": "^6.0.7",
    "@types/plotly.js": "^1.54.2",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-plotly.js": "^2.2.4",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cz-conventional-changelog": "3.3.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "wait-on": "^5.2.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.10.3",
    "worker-loader": "^3.0.5"
  },
  "dependencies": {
    "@matejmazur/react-katex": "^3.1.3",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "katex": "^0.12.0",
    "mathjs": "^8.0.0",
    "plotly.js": "^1.57.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-plotly.js": "^2.5.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "public.app-category.video"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  }
}
